        <activity
            android:name=".DisplayRestaurants"
            android:label="ManeBenefits">
        </activity>
        <activity
            android:name=".DisplayApparel"
            android:label="ManeBenefits">
        </activity>
        <activity
            android:name=".DisplayServices"
            android:label="ManeBenefits">
        </activity>
        <activity
            android:name=".DisplayActivities"
            android:label="ManeBenefits">
        </activity>
        <activity
            android:name=".DisplayHealthcare"
            android:label="ManeBenefits">
        </activity>
        <activity
            android:name=".DisplayRentals"
            android:label="ManeBenefits">
